from nltk.corpus.reader.api import *
from nltk.corpus.reader.util import *
from _typeshed import Incomplete
from nltk.tag import map_tag as map_tag, str2tuple as str2tuple

class SwitchboardTurn(list):
    """
    A specialized list object used to encode switchboard utterances.
    The elements of the list are the words in the utterance; and two
    attributes, ``speaker`` and ``id``, are provided to retrieve the
    spearker identifier and utterance id.  Note that utterance ids
    are only unique within a given discourse.
    """
    speaker: Incomplete
    id: Incomplete
    def __init__(self, words, speaker, id) -> None: ...

class SwitchboardCorpusReader(CorpusReader):
    def __init__(self, root, tagset: Incomplete | None = None) -> None: ...
    def words(self): ...
    def tagged_words(self, tagset: Incomplete | None = None): ...
    def turns(self): ...
    def tagged_turns(self, tagset: Incomplete | None = None): ...
    def discourses(self): ...
    def tagged_discourses(self, tagset: bool = False): ...
