from _typeshed import Incomplete
from nltk.corpus.reader.api import CategorizedCorpusReader as CategorizedCorpusReader
from nltk.corpus.reader.plaintext import PlaintextCorpusReader as PlaintextCorpusReader
from nltk.corpus.reader.util import concat as concat, read_blankline_block as read_blankline_block
from nltk.tokenize import blankline_tokenize as blankline_tokenize, sent_tokenize as sent_tokenize, word_tokenize as word_tokenize
from typing import NamedTuple

def comma_separated_string_args(func):
    """
    A decorator that allows a function to be called with
    a single string of comma-separated values which become
    individual function arguments.
    """
def read_parse_blankline_block(stream, parser): ...

class MarkdownBlock:
    content: Incomplete
    truncate_at: int
    def __init__(self, content) -> None: ...
    @property
    def raw(self): ...
    @property
    def words(self): ...
    @property
    def sents(self): ...
    @property
    def paras(self): ...

class CodeBlock(MarkdownBlock):
    language: Incomplete
    def __init__(self, language, *args) -> None: ...
    @property
    def sents(self): ...
    @property
    def lines(self): ...
    @property
    def paras(self): ...

class MarkdownSection(MarkdownBlock):
    heading: Incomplete
    level: Incomplete
    def __init__(self, heading, level, *args) -> None: ...

class Image(NamedTuple):
    label: Incomplete
    src: Incomplete
    title: Incomplete

class Link(NamedTuple):
    label: Incomplete
    href: Incomplete
    title: Incomplete

class List(NamedTuple):
    is_ordered: Incomplete
    items: Incomplete

class MarkdownCorpusReader(PlaintextCorpusReader):
    parser: Incomplete
    def __init__(self, *args, parser: Incomplete | None = None, **kwargs) -> None: ...

class CategorizedMarkdownCorpusReader(CategorizedCorpusReader, MarkdownCorpusReader):
    """
    A reader for markdown corpora whose documents are divided into
    categories based on their file identifiers.

    Based on nltk.corpus.reader.plaintext.CategorizedPlaintextCorpusReader:
    https://www.nltk.org/_modules/nltk/corpus/reader/api.html#CategorizedCorpusReader
    """
    def __init__(self, *args, cat_field: str = 'tags', **kwargs) -> None:
        """
        Initialize the corpus reader. Categorization arguments
        (``cat_pattern``, ``cat_map``, and ``cat_file``) are passed to
        the ``CategorizedCorpusReader`` constructor.  The remaining arguments
        are passed to the ``MarkdownCorpusReader`` constructor.
        """
    def categories(self, fileids: Incomplete | None = None): ...
    def fileids(self, categories: Incomplete | None = None): ...
    def raw(self, fileids: Incomplete | None = None, categories: Incomplete | None = None): ...
    def words(self, fileids: Incomplete | None = None, categories: Incomplete | None = None): ...
    def sents(self, fileids: Incomplete | None = None, categories: Incomplete | None = None): ...
    def paras(self, fileids: Incomplete | None = None, categories: Incomplete | None = None): ...
    def concatenated_view(self, reader, fileids, categories): ...
    def metadata_reader(self, stream): ...
    def metadata(self, fileids: Incomplete | None = None, categories: Incomplete | None = None): ...
    def blockquote_reader(self, stream): ...
    def blockquotes(self, fileids: Incomplete | None = None, categories: Incomplete | None = None): ...
    def code_block_reader(self, stream): ...
    def code_blocks(self, fileids: Incomplete | None = None, categories: Incomplete | None = None): ...
    def image_reader(self, stream): ...
    def images(self, fileids: Incomplete | None = None, categories: Incomplete | None = None): ...
    def link_reader(self, stream): ...
    def links(self, fileids: Incomplete | None = None, categories: Incomplete | None = None): ...
    def list_reader(self, stream): ...
    def lists(self, fileids: Incomplete | None = None, categories: Incomplete | None = None): ...
    def section_reader(self, stream): ...
    def sections(self, fileids: Incomplete | None = None, categories: Incomplete | None = None): ...
