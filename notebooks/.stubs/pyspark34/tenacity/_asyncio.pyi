import typing as t
from tenacity import AttemptManager as AttemptManager, BaseRetrying as BaseRetrying, DoAttempt as DoAttempt, DoSleep as DoSleep, RetryCallState as RetryCallState

WrappedFnReturnT = t.TypeVar('WrappedFnReturnT')
WrappedFn = t.TypeVar('WrappedFn', bound=t.Callable[..., t.Awaitable[t.Any]])

class AsyncRetrying(BaseRetrying):
    sleep: t.Callable[[float], t.Awaitable[t.Any]]
    def __init__(self, sleep: t.Callable[[float], t.Awaitable[t.Any]] = ..., **kwargs: t.Any) -> None: ...
    async def __call__(self, fn: WrappedFn, *args: t.Any, **kwargs: t.Any) -> WrappedFnReturnT: ...
    def __iter__(self) -> t.Generator[AttemptManager, None, None]: ...
    def __aiter__(self) -> AsyncRetrying: ...
    async def __anext__(self) -> AttemptManager: ...
    def wraps(self, fn: WrappedFn) -> WrappedFn: ...
