import unittest

class TextTestResult(unittest.TextTestResult):
    """TestResult class that provides the default text result formatting."""
    def __init__(self, stream, descriptions, verbosity) -> None: ...
    def startTest(self, test) -> None: ...
    def addSuccess(self, test) -> None: ...
    def addError(self, test, err) -> None: ...
    def addFailure(self, test, err) -> None: ...
    def addSkip(self, test, reason) -> None: ...
    def addExpectedFailure(self, test, err) -> None: ...
    def addUnexpectedSuccess(self, test) -> None: ...

class TextTestRunner(unittest.TextTestRunner):
    """A test runner that produces formatted text results."""
    run_for_debugging: bool
    def run(self, test: TestCase) -> TestResult: ...
