from dataclasses import dataclass
from nni.experiment.config.base import ConfigBase as ConfigBase
from typing_extensions import Literal

@dataclass
class PrunerConfig(ConfigBase):
    """
    Use to config the initialization parameters of a quantizer used in the compression experiment.
    """
    pruner_type: Literal['Pruner']
    def json(self): ...
    def __init__(self, pruner_type) -> None: ...

@dataclass
class L1NormPrunerConfig(PrunerConfig):
    pruner_type: Literal['L1NormPruner'] = ...
    mode: Literal['normal', 'dependency_aware'] = ...
    def __init__(self, pruner_type, mode) -> None: ...

@dataclass
class TaylorFOWeightPrunerConfig(PrunerConfig):
    pruner_type: Literal['TaylorFOWeightPruner'] = ...
    mode: Literal['normal', 'dependency_aware', 'global'] = ...
    training_batches: int = ...
    def __init__(self, pruner_type, mode, training_batches) -> None: ...
