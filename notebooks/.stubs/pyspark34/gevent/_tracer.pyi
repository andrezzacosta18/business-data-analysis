from _typeshed import Incomplete

__all__ = ['GreenletTracer', 'HubSwitchTracer', 'MaxSwitchTracer']

class GreenletTracer:
    greenlet_switch_counter: int
    active_greenlet: Incomplete
    previous_trace_function: Incomplete
    def __init__(self) -> None: ...
    def kill(self) -> None: ...
    def __call__(self, event, args): ...
    def did_block_hub(self, hub): ...
    def ignore_current_greenlet_blocking(self) -> None: ...
    def monitor_current_greenlet_blocking(self) -> None: ...
    def did_block_hub_report(self, hub, active_greenlet, format_kwargs): ...

class _HubTracer(GreenletTracer):
    max_blocking_time: Incomplete
    hub: Incomplete
    def __init__(self, hub, max_blocking_time) -> None: ...
    def kill(self) -> None: ...

class HubSwitchTracer(_HubTracer):
    last_entered_hub: int
    def __init__(self, hub, max_blocking_time) -> None: ...
    def did_block_hub(self, hub): ...

class MaxSwitchTracer(_HubTracer):
    last_switch: Incomplete
    max_blocking: int
    def __init__(self, hub, max_blocking_time) -> None: ...
    def did_block_hub(self, hub): ...
