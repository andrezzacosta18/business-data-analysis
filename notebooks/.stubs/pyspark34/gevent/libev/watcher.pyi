from _typeshed import Incomplete
from gevent._ffi import watcher as _base

ffi: Incomplete
libev: Incomplete
vfd_open: Incomplete
vfd_free: Incomplete
vfd_get: Incomplete

class watcher(_base.watcher):
    def __init__(self, _loop, ref: bool = True, priority: Incomplete | None = None, args=...) -> None: ...
    ref: Incomplete
    priority: Incomplete
    callback: Incomplete
    args: Incomplete
    def feed(self, revents, callback, *args) -> None: ...
    @property
    def pending(self): ...

class io(_base.IoMixin, watcher):
    EVENT_MASK: Incomplete
    fd: Incomplete
    events: Incomplete
    @property
    def events_str(self): ...

class timer(_base.TimerMixin, watcher):
    @property
    def at(self): ...
    def again(self, callback, *args, **kw) -> None: ...

class signal(_base.SignalMixin, watcher): ...
class idle(_base.IdleMixin, watcher): ...
class prepare(_base.PrepareMixin, watcher): ...
class check(_base.CheckMixin, watcher): ...
class fork(_base.ForkMixin, watcher): ...

class async_(_base.AsyncMixin, watcher):
    def send(self) -> None: ...
    @property
    def pending(self): ...

class _ClosedWatcher:
    pid: Incomplete
    rpid: Incomplete
    rstatus: Incomplete
    def __init__(self, other) -> None: ...
    def __bool__(self) -> bool: ...
    __nonzero__ = __bool__

class child(_base.ChildMixin, watcher):
    def close(self) -> None: ...
    @property
    def pid(self): ...
    @property
    def rpid(self): ...
    @rpid.setter
    def rpid(self, value) -> None: ...
    @property
    def rstatus(self): ...
    @rstatus.setter
    def rstatus(self, value) -> None: ...

class stat(_base.StatMixin, watcher):
    @property
    def attr(self): ...
    @property
    def prev(self): ...
    @property
    def interval(self): ...
