from .Compiler.ExprNodes import *
from .Compiler.Nodes import CSimpleBaseTypeNode as CSimpleBaseTypeNode
from .Compiler.Visitor import TreeVisitor as TreeVisitor
from _typeshed import Incomplete

class LinesResult:
    lines: Incomplete
    s: str
    def __init__(self) -> None: ...
    def put(self, s) -> None: ...
    def newline(self) -> None: ...
    def putline(self, s) -> None: ...

class DeclarationWriter(TreeVisitor):
    """
    A Cython code writer that is limited to declarations nodes.
    """
    indent_string: str
    result: Incomplete
    numindents: int
    tempnames: Incomplete
    tempblockindex: int
    def __init__(self, result: Incomplete | None = None) -> None: ...
    def write(self, tree): ...
    def indent(self) -> None: ...
    def dedent(self) -> None: ...
    def startline(self, s: str = '') -> None: ...
    def put(self, s) -> None: ...
    def putline(self, s) -> None: ...
    def endline(self, s: str = '') -> None: ...
    def line(self, s) -> None: ...
    def comma_separated_list(self, items, output_rhs: bool = False) -> None: ...
    def visit_Node(self, node) -> None: ...
    def visit_ModuleNode(self, node) -> None: ...
    def visit_StatListNode(self, node) -> None: ...
    def visit_CDefExternNode(self, node) -> None: ...
    def visit_CPtrDeclaratorNode(self, node) -> None: ...
    def visit_CReferenceDeclaratorNode(self, node) -> None: ...
    def visit_CArrayDeclaratorNode(self, node) -> None: ...
    def visit_CFuncDeclaratorNode(self, node) -> None: ...
    def visit_CNameDeclaratorNode(self, node) -> None: ...
    def visit_CSimpleBaseTypeNode(self, node) -> None: ...
    def visit_CComplexBaseTypeNode(self, node) -> None: ...
    def visit_CNestedBaseTypeNode(self, node) -> None: ...
    def visit_TemplatedTypeNode(self, node) -> None: ...
    def visit_CVarDefNode(self, node) -> None: ...
    def visit_CStructOrUnionDefNode(self, node) -> None: ...
    def visit_CppClassNode(self, node) -> None: ...
    def visit_CEnumDefNode(self, node) -> None: ...
    def visit_CEnumDefItemNode(self, node) -> None: ...
    def visit_CClassDefNode(self, node) -> None: ...
    def visit_CTypeDefNode(self, node) -> None: ...
    def visit_FuncDefNode(self, node) -> None: ...
    def visit_CFuncDefNode(self, node) -> None: ...
    def visit_CArgDeclNode(self, node) -> None: ...
    def visit_CImportStatNode(self, node) -> None: ...
    def visit_FromCImportStatNode(self, node) -> None: ...
    def visit_NameNode(self, node) -> None: ...
    def visit_DecoratorNode(self, node) -> None: ...
    def visit_PassStatNode(self, node) -> None: ...

class StatementWriter(DeclarationWriter):
    """
    A Cython code writer for most language statement features.
    """
    def visit_SingleAssignmentNode(self, node) -> None: ...
    def visit_CascadedAssignmentNode(self, node) -> None: ...
    def visit_PrintStatNode(self, node) -> None: ...
    def visit_ForInStatNode(self, node) -> None: ...
    def visit_IfStatNode(self, node) -> None: ...
    def visit_WhileStatNode(self, node) -> None: ...
    def visit_ContinueStatNode(self, node) -> None: ...
    def visit_BreakStatNode(self, node) -> None: ...
    def visit_SequenceNode(self, node) -> None: ...
    def visit_ExprStatNode(self, node) -> None: ...
    def visit_InPlaceAssignmentNode(self, node) -> None: ...
    def visit_WithStatNode(self, node) -> None: ...
    def visit_TryFinallyStatNode(self, node) -> None: ...
    def visit_TryExceptStatNode(self, node) -> None: ...
    def visit_ExceptClauseNode(self, node) -> None: ...
    def visit_ReturnStatNode(self, node) -> None: ...
    def visit_ReraiseStatNode(self, node) -> None: ...
    def visit_ImportNode(self, node) -> None: ...
    def visit_TempsBlockNode(self, node) -> None:
        """
        Temporaries are output like $1_1', where the first number is
        an index of the TempsBlockNode and the second number is an index
        of the temporary which that block allocates.
        """
    def visit_TempRefNode(self, node) -> None: ...

class ExpressionWriter(TreeVisitor):
    """
    A Cython code writer that is intentionally limited to expressions.
    """
    result: Incomplete
    precedence: Incomplete
    def __init__(self, result: Incomplete | None = None) -> None: ...
    def write(self, tree): ...
    def put(self, s) -> None: ...
    def remove(self, s) -> None: ...
    def comma_separated_list(self, items) -> None: ...
    def visit_Node(self, node) -> None: ...
    def visit_IntNode(self, node) -> None: ...
    def visit_FloatNode(self, node) -> None: ...
    def visit_NoneNode(self, node) -> None: ...
    def visit_NameNode(self, node) -> None: ...
    def visit_EllipsisNode(self, node) -> None: ...
    def visit_BoolNode(self, node) -> None: ...
    def visit_ConstNode(self, node) -> None: ...
    def visit_ImagNode(self, node) -> None: ...
    def emit_string(self, node, prefix: str = '') -> None: ...
    def visit_BytesNode(self, node) -> None: ...
    def visit_StringNode(self, node) -> None: ...
    def visit_UnicodeNode(self, node) -> None: ...
    def emit_sequence(self, node, parens=('', '')) -> None: ...
    def visit_ListNode(self, node) -> None: ...
    def visit_TupleNode(self, node) -> None: ...
    def visit_SetNode(self, node) -> None: ...
    def visit_DictNode(self, node) -> None: ...
    def visit_DictItemNode(self, node) -> None: ...
    unop_precedence: Incomplete
    binop_precedence: Incomplete
    def operator_enter(self, new_prec) -> None: ...
    def operator_exit(self) -> None: ...
    def visit_NotNode(self, node) -> None: ...
    def visit_UnopNode(self, node) -> None: ...
    def visit_BinopNode(self, node) -> None: ...
    def visit_BoolBinopNode(self, node) -> None: ...
    def visit_PrimaryCmpNode(self, node) -> None: ...
    def visit_IndexNode(self, node) -> None: ...
    def visit_SliceIndexNode(self, node) -> None: ...
    def visit_SliceNode(self, node) -> None: ...
    def visit_CondExprNode(self, node) -> None: ...
    def visit_AttributeNode(self, node) -> None: ...
    def visit_SimpleCallNode(self, node) -> None: ...
    def emit_pos_args(self, node) -> None: ...
    def emit_kwd_args(self, node) -> None: ...
    def visit_GeneralCallNode(self, node) -> None: ...
    def emit_comprehension(self, body, target, sequence, condition, parens=('', '')) -> None: ...
    def visit_ComprehensionAppendNode(self, node) -> None: ...
    def visit_DictComprehensionAppendNode(self, node) -> None: ...
    def visit_ComprehensionNode(self, node) -> None: ...
    def visit_GeneratorExpressionNode(self, node) -> None: ...

class PxdWriter(DeclarationWriter, ExpressionWriter):
    """
    A Cython code writer for everything supported in pxd files.
    (currently unused)
    """
    def __call__(self, node): ...
    def visit_CFuncDefNode(self, node) -> None: ...
    def visit_StatNode(self, node) -> None: ...

class CodeWriter(StatementWriter, ExpressionWriter):
    """
    A complete Cython code writer.
    """
