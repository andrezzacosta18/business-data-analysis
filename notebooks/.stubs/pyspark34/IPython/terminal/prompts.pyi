from IPython.core.displayhook import DisplayHook as DisplayHook
from _typeshed import Incomplete

class Prompts:
    shell: Incomplete
    def __init__(self, shell) -> None: ...
    def vi_mode(self): ...
    def in_prompt_tokens(self): ...
    def continuation_prompt_tokens(self, width: Incomplete | None = None): ...
    def rewrite_prompt_tokens(self): ...
    def out_prompt_tokens(self): ...

class ClassicPrompts(Prompts):
    def in_prompt_tokens(self): ...
    def continuation_prompt_tokens(self, width: Incomplete | None = None): ...
    def rewrite_prompt_tokens(self): ...
    def out_prompt_tokens(self): ...

class RichPromptDisplayHook(DisplayHook):
    """Subclass of base display hook using coloured prompt"""
    prompt_end_newline: bool
    def write_output_prompt(self) -> None: ...
    def write_format_data(self, format_dict, md_dict: Incomplete | None = None) -> None: ...
