import ast
from _typeshed import Incomplete
from typing import NamedTuple

class ExternalReference(NamedTuple):
    name: Incomplete
    node: Incomplete
    name_ref: Incomplete

class ScopeVisitor(ast.NodeVisitor):
    root_scope: Incomplete
    def __init__(self) -> None: ...
    def visit(self, node) -> None: ...
    def visit_in_order(self, node, *attrs) -> None: ...
    def visit_Import(self, node) -> None: ...
    def visit_ImportFrom(self, node) -> None: ...
    def visit_Name(self, node) -> None: ...
    scope: Incomplete
    def visit_FunctionDef(self, node) -> None: ...
    def visit_arguments(self, node) -> None: ...
    def visit_arg(self, node) -> None: ...
    def visit_ClassDef(self, node) -> None: ...
    def visit_Attribute(self, node) -> None: ...

class Scope:
    parent_scope: Incomplete
    names: Incomplete
    node: Incomplete
    def __init__(self, parent_scope, node) -> None: ...
    def define_name(self, name, node): ...
    def lookup_name(self, name): ...
    def get_root_scope(self): ...
    def lookup_scope(self, node): ...
    def create_scope(self, node): ...

class RootScope(Scope):
    external_references: Incomplete
    def __init__(self, node) -> None: ...
    def add_external_reference(self, name, node, name_ref: Incomplete | None = None) -> None: ...
    def get_root_scope(self): ...
    def parent(self, node): ...
    def set_parent(self, node, parent) -> None: ...
    def get_name_for_node(self, node): ...
    def set_name_for_node(self, node, name) -> None: ...
    def lookup_scope(self, node): ...

class Name:
    id: Incomplete
    definition: Incomplete
    reads: Incomplete
    attrs: Incomplete
    def __init__(self, id) -> None: ...
    def add_reference(self, node) -> None: ...
    def define(self, node) -> None: ...
    def lookup_name(self, name): ...

def analyze(tree): ...
