from _typeshed import Incomplete
from collections.abc import Generator
from typing import IO

logger: Incomplete

class TrainingLogRecord:
    record_id: Incomplete
    iter_per_learner: Incomplete
    logged_metric: Incomplete
    trial_time: Incomplete
    wall_clock_time: Incomplete
    validation_loss: Incomplete
    config: Incomplete
    learner: Incomplete
    sample_size: Incomplete
